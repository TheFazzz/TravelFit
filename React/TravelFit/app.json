{
  "expo": {
    "name": "TravelFit",
    "slug": "travelfit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.phera.travelfit",
      "config": {
        "googleMapsApiKey": "AIzaSyCxMQEBsj8-dfRXKdbAaNM2fV-mPxZvLEk"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.phera.travelfit",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD3G2cNCeur4gjDXFf7oKaMqtSakeFGNCE"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow TravelFit to access your camera",
          "microphonePermission": "Allow TravelFit to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0afcd4fb-8726-4c76-8d66-0f7d3b229d15"
      }
    },
    "owner": "phera"
  }
}
